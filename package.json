{
  "name": "@torixtv/nestjs-inngest",
  "version": "0.11.4",
  "description": "Modern NestJS integration for Inngest - type-safe, decorator-based event-driven functions",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "require": "./dist/testing.js",
      "import": "./dist/testing.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nest build",
    "build:watch": "nest build --watch",
    "dev": "nest build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config test/jest-e2e.json",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run build",
    "start:e2e": "ts-node -P test/e2e/tsconfig.app.json test/e2e/app/main.ts",
    "start:e2e:dev": "nodemon --exec ts-node -P test/e2e/tsconfig.app.json test/e2e/app/main.ts"
  },
  "keywords": [
    "nestjs",
    "inngest",
    "event-driven",
    "serverless",
    "background-jobs",
    "typescript",
    "decorators",
    "queue",
    "workflow",
    "microservices",
    "async",
    "jobs",
    "scheduler",
    "cron",
    "retry",
    "resilient",
    "durable-functions",
    "step-functions",
    "event-sourcing",
    "messaging",
    "webhooks",
    "observability",
    "tracing",
    "monitoring"
  ],
  "author": "Dan Shapir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/torixtv/nestjs-inngest.git"
  },
  "bugs": {
    "url": "https://github.com/torixtv/nestjs-inngest/issues"
  },
  "homepage": "https://github.com/torixtv/nestjs-inngest#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@opentelemetry/auto-instrumentations-node": "^0.56.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "inngest": "^3.49.1",
    "jest": "^30.1.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/terminus": "^11.0.0",
    "express": "^5.1.0",
    "inngest": "^3.49.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^3.25.76"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": true
    },
    "@opentelemetry/sdk-node": {
      "optional": true
    },
    "@nestjs/terminus": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.56.0"
  },
  "dependencies": {
    "@torixtv/nestjs-inngest": "^0.11.3",
    "uuid": "^11.1.0"
  }
}
